{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-node",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        },
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.mjs",
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "scripts": {
    "deploy": "echo Deploy not allowed!",
    "deploy3": "echo Deploy3 not allowed!",
    "start": "nodemon",
    "start-cjs": "node ./dist/node/index.cjs",
    "start-esm": "node ./dist/node/index.mjs",
    "test": "jest --no-cache --forceExit",
    "test:ci": "jest --runInBand --coverage --forceExit --passWithNoTests"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.623.0",
    "@xylabs/assert": "^3.6.8",
    "@xyo-network/node-app": "^2.89.1",
    "axios": "^1.7.3",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "node-cache": "^5.1.2",
    "pako": "^2.1.0",
    "rollbar": "^2.26.4",
    "winston": "^3.13.1",
    "winston-transport": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.14",
    "@types/source-map-support": "0.5.10",
    "@xylabs/eslint-config": "^3.15.5",
    "@xylabs/eslint-config-flat": "^3.15.5",
    "@xylabs/ts-scripts-yarn3": "^3.15.5",
    "@xylabs/tsconfig": "^3.15.5",
    "@xylabs/tsconfig-jest": "^3.15.5",
    "babel-jest": "^29.7.0",
    "depcheck": "^1.4.7",
    "eslint": "~9.8.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "resolutions": {
    "quick-lru": "5.1.1"
  },
  "packageManager": "yarn@4.4.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.5.1",
    "yarn": "4.4.0"
  },
  "type": "commonjs"
}
