{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "api-archivist-nodejs",
  "version": "2.0.96",
  "description": "XYO Archivist API",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm5/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm5/index.js",
      "node": {
        "import": "./dist/node-esm/index.js",
        "require": "./dist/node/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/node/index.js",
    "clean": "rimraf tsconfig.tsbuildinfo && rimraf ./build && rimraf ./.serverless && rimraf ./.webpack",
    "compile": "yarn gitlint-fix && rimraf ./dist && yarn compile-rollup",
    "compile-rollup": "rollup -c rollup.config.js",
    "compile-tsc": "tsc -p tsconfig.build.json",
    "reinstall": "rimraf ./yarn-error.log && rimraf ./node_modules && yarn install && yarn outdated",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\"",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.85.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@xylabs/sdk-api-express-ecs": "^1.1.4",
    "@xylabs/sdk-js": "^2.5.5",
    "@xyo-network/sdk-xyo-client-js": "^2.20.8",
    "@xyo-network/sdk-xyo-mongo-js": "^2.2.7",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bn.js": "^5.2.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.5",
    "express": "^4.18.1",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.9.0",
    "mongodb": "^4.5.0",
    "node-cache": "^5.1.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rollbar": "^2.25.0",
    "rollup": "^2.72.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "sha.js": "^2.4.11",
    "source-map-support": "^0.5.21",
    "swagger-autogen": "^2.21.1",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/bn.js": "^5.1.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/express-mung": "^0.5.2",
    "@types/jest": "^27.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/lru-cache": "^7.6.1",
    "@types/node": "^17.0.31",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/source-map-support": "0.5.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "babel-jest": "^28.0.3",
    "depcheck": "^1.4.3",
    "elliptic": "^6.5.4",
    "jest": "^28.0.3",
    "jest-sorted": "^1.0.14",
    "license-checker": "^25.0.1",
    "nodemon": "^2.0.16",
    "npm-check-updates": "^12.5.11",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.6.4"
  },
  "packageManager": "yarn@3.1.1"
}
