{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "clean": "rimraf tsconfig.tsbuildinfo && rimraf ./build && rimraf ./.serverless && rimraf ./.webpack",
    "compile": "yarn gitlint-fix && rimraf ./dist && yarn compile:esm && yarn compile:cjs",
    "compile:cjs": "tsc -p tsconfig.build.cjs.json",
    "compile:esm": "tsc -p tsconfig.build.esm.json",
    "reinstall": "rimraf ./yarn-error.log && rimraf ./node_modules && yarn install && yarn outdated",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\"",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^1.2.4",
    "@xylabs/sdk-js": "^2.6.2",
    "@xyo-network/account": "^2.21.19",
    "@xyo-network/sdk-xyo-client-js": "^2.21.19",
    "@xyo-network/sdk-xyo-mongo-js": "^2.2.9",
    "ajv": "^8.11.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "express": "^4.18.1",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.25.0",
    "source-map-support": "^0.5.21",
    "swagger-autogen": "^2.21.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/express-mung": "^0.5.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/source-map-support": "0.5.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@xylabs/eslint-config": "^2.3.30",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.14",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "jest-sorted": "^1.0.14",
    "license-checker": "^25.0.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.1.1",
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  }
}
