{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "api-archivist-nodejs",
  "version": "2.0.96",
  "description": "XYO Archivist API",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm5/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm5/index.js",
      "node": {
        "import": "./dist/node-esm/index.js",
        "require": "./dist/node/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "nodemon --ext .ts --watch ./src --exec \"node --max-old-space-size=8192 --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts\"",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/node-esm/index.js",
    "build": "yarn lint && yarn compile",
    "clean": "rimraf tsconfig.tsbuildinfo && rimraf ./build && rimraf ./.serverless && rimraf ./.webpack",
    "compile": "rimraf ./dist && yarn compile-rollup",
    "compile-rollup": "rollup -c rollup-config/index.js",
    "compile-tsc": "tsc -p tsconfig.build.json",
    "reinstall": "rimraf ./yarn-error.log && rimraf ./node_modules && yarn install && yarn outdated",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.43.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@xylabs/sdk-api-express-ecs": "^1.0.4",
    "@xylabs/sdk-js": "^2.3.19",
    "@xyo-network/sdk-xyo-client-js": "^2",
    "@xyo-network/sdk-xyo-mongo-js": "^2.2.1",
    "axios": "^0.24.0",
    "bn.js": "^5.2.0",
    "body-parser": "^1.19.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "mongodb": "^4.2.2",
    "node-cache": "^5.1.2",
    "rollbar": "^2.24.0",
    "rollup": "^2.61.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "sha.js": "^2.4.11",
    "source-map-support": "^0.5.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/bn.js": "^5.1.0",
    "@types/express": "4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17",
    "@types/source-map-support": "0.5.4",
    "@xylabs/eslint-config": "^2.2.4",
    "@xylabs/ts-scripts": "^1.0.24",
    "@xylabs/tsconfig": "^1.0.5",
    "depcheck": "^1.4.2",
    "elliptic": "^6.5.4",
    "eslint": "^8.4.1",
    "license-checker": "^25.0.1",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4"
  },
  "type": "module"
}
