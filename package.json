{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-node",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        },
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.js"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "scripts": {
    "deploy": "echo Deploy not allowed!",
    "deploy3": "echo Deploy3 not allowed!",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/node/index.js",
    "start": "nodemon",
    "start-cjs": "node ./dist/node/index.js",
    "start-esm": "node ./dist/node/index.mjs",
    "test": "jest --no-cache --forceExit",
    "test:ci": "jest --runInBand --coverage --forceExit --passWithNoTests"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.556.0",
    "@xylabs/assert": "^3.1.11",
    "@xyo-network/node-app": "^2.87.2",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "node-cache": "^5.1.2",
    "pako": "^2.1.0",
    "rollbar": "^2.26.4",
    "winston": "^3.13.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/source-map-support": "0.5.10",
    "@xylabs/eslint-config": "^3.9.1",
    "@xylabs/ts-scripts-yarn3": "^3.9.1",
    "@xylabs/tsconfig": "^3.9.1",
    "babel-jest": "^29.7.0",
    "depcheck": "^1.4.7",
    "eslint": "~8.57.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "quick-lru": "5.1.1"
  },
  "packageManager": "yarn@4.1.1",
  "private": true,
  "engines": {
    "node": ">=20.10.0 <21"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "1.22.19"
  },
  "type": "commonjs"
}
