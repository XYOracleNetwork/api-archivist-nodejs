{
  "name": "api-archivist-nodejs",
  "version": "2.0.75",
  "description": "XYO Archivist Serverless API",
  "sideeffects": false,
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "lint": "yarn eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "yarn eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "cycle": "NODE_ENV=--max-old-space-size=8092 eslint --rule 'import/no-cycle: [1, { maxDepth: 10 }]' ./src",
    "compile": "yarn lint && yarn tsc",
    "watch": "yarn tsc --watch",
    "build": "yarn version --patch --no-git-tag-version && yarn build:ci",
    "build:ci": "yarn lint && yarn compile && yarn serverless package",
    "build:prod": "yarn compile && yarn serverless package --stage prod",
    "start": "yarn compile && yarn serverless offline --httpPort 3030 --useChildProcesses --useWorkerThreads",
    "start:prod": "yarn compile && yarn serverless offline --httpPort 3030 --stage prod --useChildProcesses --useWorkerThreads",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -r -f ./build && rm -r -f ./.serverless && rm -r -f ./.webpack",
    "deploy:sls": "NODE_OPTIONS=--max_old_space_size=8192 yarn serverless deploy --stage beta",
    "deploy:sls:prod": "yarn serverless deploy --stage prod",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "license": "yarn license-checker --exclude 'MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0'",
    "up": "yarn ncu",
    "updo": "yarn upgrade && yarn ncu -u && yarn install",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck"
  },
  "author": "Arie Trouw",
  "license": "ISC",
  "dependencies": {
    "@xylabs/eslint-config": "^2.0.20",
    "@xyo-network/sdk-xyo-client-js": "^2.1.53",
    "@xyo-network/sdk-xyo-js": "^2.1.43",
    "@xyo-network/sdk-xyo-mongo-js": "^2.1.18",
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.3",
    "dotenv": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.1",
    "sha.js": "^2.4.11",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16",
    "@types/source-map-support": "0.5.4",
    "depcheck": "^1.4.2",
    "elliptic": "^6.5.4",
    "eslint": "^7.32.0",
    "license-checker": "^25.0.1",
    "npm-check-updates": "^11.8.3",
    "serverless": "^2.56.0",
    "serverless-api-gateway-caching": "^1.7.4",
    "serverless-bundle": "^5.0.0",
    "serverless-domain-manager": "^5.1.5",
    "serverless-offline": "^8.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "4.3.5"
  }
}
