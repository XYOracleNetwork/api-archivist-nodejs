{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-node",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "deploy": "echo Deploy not allowed!",
    "deploy3": "echo Deploy3 not allowed!",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "start": "nodemon",
    "test": "jest --no-cache --forceExit",
    "test:ci": "jest --runInBand --coverage --forceExit --passWithNoTests"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.267.0",
    "@xylabs/assert": "^2.7.1",
    "@xyo-network/node-app": "^2.43.36",
    "axios": "^1.3.3",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "node-cache": "^5.1.2",
    "pako": "^2.1.0",
    "rollbar": "^2.26.1",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/source-map-support": "0.5.6",
    "@xylabs/eslint-config": "^2.14.14",
    "@xylabs/ts-scripts-yarn3": "^2.14.14",
    "@xylabs/tsconfig": "^2.14.14",
    "@xyo-network/account": "^2.43.36",
    "@xyo-network/boundwitness": "^2.43.36",
    "@xyo-network/payload": "^2.43.36",
    "babel-jest": "^29.4.2",
    "depcheck": "^1.4.3",
    "eslint": "~8.34.0",
    "jest": "^29.4.2",
    "jest-extended": "^3.2.3",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "quick-lru": "5.1.1"
  },
  "packageManager": "yarn@3.3.1",
  "private": true,
  "engines": {
    "node": ">=18.13.0 <19"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  }
}
