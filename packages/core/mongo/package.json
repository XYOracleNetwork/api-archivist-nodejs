{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-mongo",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-js": "^2.6.7",
    "@xyo-network/account": "^2.27.0-rc.6",
    "@xyo-network/archivist-model": "workspace:^",
    "@xyo-network/archivist-types": "workspace:^",
    "@xyo-network/diviner": "^2.27.0-rc.6",
    "@xyo-network/sdk-xyo-client-js": "^2.27.0-rc.6",
    "@xyo-network/sdk-xyo-mongo-js": "^2.2.9",
    "inversify": "^6.0.1",
    "mongodb": "^4.9.0",
    "reflect-metadata": "^0.1.13"
  },
  "private": true,
  "types": "dist/esm/index.d.ts"
}
