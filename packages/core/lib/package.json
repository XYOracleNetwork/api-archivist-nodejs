{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-lib",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^1.2.4",
    "@xylabs/sdk-js": "^2.6.3",
    "@xyo-network/account": "^2.23.3",
    "@xyo-network/archivist-model": "workspace:^",
    "@xyo-network/sdk-xyo-client-js": "^2.23.3",
    "@xyo-network/sdk-xyo-mongo-js": "^2.2.9",
    "ajv": "^8.11.0",
    "inversify": "^6.0.1",
    "mongodb": "^4.8.1",
    "passport": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/passport": "^1.0.9",
    "@types/source-map-support": "0.5.4",
    "uuid": "^8.3.2"
  },
  "private": true,
  "types": "dist/esm/index.d.ts"
}
