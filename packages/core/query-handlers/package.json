      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/assert": "^2.6.9",
    "@xylabs/delay": "^2.6.9",
    "@xyo-network/archivist": "^2.36.0-rc.2",
    "@xyo-network/archivist-model": "workspace:^",
    "@xyo-network/archivist-types": "workspace:^",
    "@xyo-network/core": "^2.36.0-rc.2",
    "@xyo-network/domain-payload-plugin": "^2.36.0-rc.2",
    "@xyo-network/payload": "^2.36.0-rc.2",
    "@xyo-network/schema-payload-plugin": "^2.36.0-rc.2",
    "@xyo-network/utils": "^2.36.0-rc.2",
    "inversify": "^6.0.1"
  },
  "devDependencies": {
    "jest": "^29.1.1",
    "jest-mock-extended": "^3.0.1",
    "typescript": "^4.8.4"
  },
  "private": true,
  "types": "dist/esm/index.d.ts"
}
