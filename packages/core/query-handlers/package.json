    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-js": "^2.6.2",
    "@xyo-network/archivist-model": "workspace:^",
    "@xyo-network/archivist-types": "workspace:^",
    "@xyo-network/sdk-xyo-client-js": "^2.22.20",
    "inversify": "^6.0.1"
  },
  "devDependencies": {
    "@xylabs/ts-scripts": "^2.5.6",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "license-checker": "^25.0.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "types": "dist/esm/index.d.ts"
