{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-dependencies",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^1.2.4",
    "@xylabs/sdk-js": "^2.6.7",
    "@xyo-network/account": "^2.31.0",
    "@xyo-network/archivist-express-lib": "workspace:^",
    "@xyo-network/archivist-middleware": "workspace:^",
    "@xyo-network/archivist-model": "workspace:^",
    "@xyo-network/archivist-mongo": "workspace:^",
    "@xyo-network/archivist-query-handlers": "workspace:^",
    "@xyo-network/archivist-queue": "workspace:^",
    "@xyo-network/archivist-types": "workspace:^",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "private": true,
  "types": "dist/esm/index.d.ts"
}
