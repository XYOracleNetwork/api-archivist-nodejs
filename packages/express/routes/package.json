{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/archivist-routes",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^1.2.4",
    "@xylabs/sdk-js": "^2.6.7",
    "@xyo-network/archivist-dependencies": "workspace:^",
    "@xyo-network/archivist-express-lib": "workspace:^",
    "@xyo-network/archivist-lib": "workspace:^",
    "@xyo-network/archivist-middleware": "workspace:^",
    "@xyo-network/archivist-model": "workspace:^",
    "@xyo-network/archivist-queue": "workspace:^",
    "@xyo-network/archivist-types": "workspace:^",
    "@xyo-network/domain-payload-plugin": "2.28.0-rc.7",
    "@xyo-network/sdk-xyo-client-js": "2.28.0-rc.7",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/source-map-support": "0.5.6",
    "@types/uuid": "^8.3.4"
  },
  "private": true,
  "types": "dist/esm/index.d.ts"
}
